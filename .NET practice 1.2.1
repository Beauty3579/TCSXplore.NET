using System;
					
public class Program
{
	public static void Main()
	{
        Console.WriteLine("Enter start value:");
		int s = Convert.ToInt32(Console.ReadLine());
		Console.WriteLine("Enter end value:");
		int e = Convert.ToInt32(Console.ReadLine());
		for(int i=s; i<e; i++)
		{
			if(i%2 == 0)
			{
				Console.WriteLine(i);
			}
		}
		Console.ReadLine();
	}
}
